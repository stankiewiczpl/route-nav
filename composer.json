{
    "name": "stankiewiczpl/route-nav",
    "description": "Laravel route based menus and breadcrumbs ",
    "keywords": [
        "laravel",
        "laravel menu",
        "laravel breadcrumbs",
        "laravel navigation"
    ],
    "homepage": "https://github.com/stankiewiczpl/route-nav",
    "authors": [
        {
            "name": "Piotr Stankiewicz",
            "email": "stankiewicz.opole@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.6|^6.0",
        "illuminate/view": "^5.6|^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Stankiewiczpl\\RouteNav\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/",
            "RouteNavTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stankiewiczpl\\RouteNav\\RouteNavServiceProvider"
            ],
            "aliases": {
                "RouteNav": "Stankiewiczpl\\RouteNav\\Facades\\RouteNav"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
